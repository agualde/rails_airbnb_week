<div class="container">
              <div class="show-header">
              <div class="d-flex">
                <h1><%= @flat.title %></h1>
                </div>
                <div class="show-header-links">
                <a href='/flats/<%= params[:id] %>/edit' class="<%= @flat.user_id == current_user ? "" : "d-none" %>" id="pencil">‚úèÔ∏è</a>
                <h3><%= link_to "üì§", dashboards_path %></h3>
                <h3><%= link_to "‚ô•", dashboards_path %></h3>
                </div>
              </div>


              <div class="show-images my-5">
                <div class="show-image-div" style="background-image: url(<%= cl_image_path @flat.photos.first.key %>);"></div>
                <div class="show-grid">
                  <% @flat.photos[1..-1].each do |photo| %>
                    <div class="class-image-ingrid" style="background-image:url(<%=cl_image_path photo.key%>);"></div>
                  <% end %>
                </div>
              </div>

              <div class="flat-titles">

            <h2>Some interesting information about the flat</h2>
            <div>
              <h6 class="my-2 head"><%= @flat.capacity %> <%= @flat.capacity == 1 ? "guest" : "guests" %> üë§</h6>
              <h6 class="my-2 head"><%= @flat.rooms %> <%= @flat.rooms == 1 ? "room" : "rooms" %> <img src="https://cdn.iconscout.com/icon/premium/png-256-thumb/available-room-1-501679.png" alt="" class="rooms"> </h6>
            </div>
              <h5 class="my-2">Address: <%= @flat.address %></h5>
            </div>

<div class="show-info">
          <div class="show-contents">
            <div class="my-show-header my-3">
            </div>

            <ul class="show-list">
              <div class="show-price-guests">
              <li class="my-2">Descrption: <%= @flat.description %></li>
              <li class="my-2">Address: <%= @flat.address %></li>
              <li class="my-2">‚Ç¨ <%= @flat.price %>/night</li>
              </div>
            </ul>

            <div class="booking-card">
              <ul class="booking-list">
              <div class="price-calc mb-3">
                <li>$<%= @flat.price %> x <%= $total.to_i %> <%= $total.to_i == 1 ? "night" : "nights" %> </li>
                <li>$<%= total = @flat.price * $total.to_i %></li>
              </div>
              <div class="price-comission mb-3">
                <li>Comission</li>
                <li>$<%= comission = (total * 0.07).round %></li>
              </div>
              <div class="total my-3">
                <li>Total</li>
                <li>$<%= total + comission %></li>
              </div>
              </ul>
              <%= link_to "Book now!", new_flat_booking_path(@flat), class:"btn center go-btn mt-3" %>
            </div>
        </div>

 <div class="flat-features">
            <h4>What this flat has to offer:</h4>
            <div class="facilities-flex">
              <div>
                <ul>
                  <li><img src="https://cdn.iconscout.com/icon/premium/png-256-thumb/kitchen-1447827-1224041.png" alt="" style="width: 26px;">  <span class="<%= @flat.kitchen ? "" : "line-through" %>">  Kitchen Facilities</span> </li>
                  <li> <%= icon('fa', 'wifi') %><span class="<%= @flat.wifi ? "" : "line-through" %>" >  Wifi</li>
                  <li> <%= icon('fa', 'smoking') %> <span class="<%= @flat.smoker_friendly ? "" : "line-through" %>" >  Smoker Friendly</li>
                  <li><%= icon('fa', 'dog') %><span class="<%= @flat.accept_pets ? "" : "line-through" %>" >  Pet Friendly</li>
                  <li><img src="https://cdn.iconscout.com/icon/premium/png-256-thumb/snow-1840405-1559531.png" alt="" style="width: 24px;"> <span class="<%= @flat.air_con ? "" : "line-through" %>">  Air-Conditioning</span></li>
                  <li><img src="https://w7.pngwing.com/pngs/363/123/png-transparent-car-park-computer-icons-parking-garage-hotel-parking-angle-room-parking.png" alt="" style="width: 28px; height: 19px">  <span>  Parking</span></li>
                </ul>
              </div>
            <div>
                <ul>
                  <li><img src="https://cdn-icons-png.flaticon.com/512/157/157839.png" alt="" style="width: 24px; height: 19px">  <span>  Pool Access</span></li>
                  <li><img src="data:image/png;base64," alt="" style="width: 24px; height: 19px">  <span>  TV</span></li>
                  <li><img src="data:image/png;base64," alt="" style="width: 24px; height: 19px"> <span>  Washer/Dryer</span></li>
                  <li><%= icon('fa', 'suitcase') %><span class="<%= @flat.luggage_drop_off ? "" : "line-through" %>" > Luggage Drop-Off</li>
                  <li><img src="data:image/png;base64," alt="" style="width: 24px; height: 19px">  <span>  Balcony</span></li>
                </ul>
            </div>
          </div>
          </div>



</div>
